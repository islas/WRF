{
  "submit_options" :
  {
    "queue"     : "economy",
    "timelimit" : "01:00:00",
    "arguments" : 
    {
      "global_env" : [ "-e", "DATA=/path/to/data" ]
    },
    "cheyenne"  :
    {
      "submission" : "PBS",
      "resources"  : "1:ncpus=8",
      "arguments"  : 
      {
        "global_env"     : 
        [ 
          "-e", "DATA=/glade/work/aislas/github/data/wrf/regtest/Data/",
          "-e", "LD_LIBRARY_PATH=$LD_LIBRARY_PATH:$NETCDF/lib"
        ],
        "global_modules" : [ "gnu", "netcdf" ]
      }
    }
  },
  "em_real_serial" :
  {
    "steps" :
    {
      "build" :
      {
        "command"      : ".ci/tests/build.sh",
        "arguments"    : [ "-c", "32$'\\n'1", "-b", "em_real -j 8" ]
      },
      "testcase" :
      {
        "submit_options" : { "cheyenne" : { "resources" : "1:ncpus=1" } },
        "command"      : ".ci/tests/runNamelists.sh",
        "arguments"    : 
        [ 
          "-c", "test/em_real",
          "-b", "real.exe",
          "-f", ".ci/tests/SCRIPTS/Namelists/weekly/em_real/SERIAL/",
          "-n", "namelist.input.3dtke,namelist.input.conus,namelist.input.rap,namelist.input.tropical",
          "-d", "$DATA/em_real"
        ],
        "dependencies" : { "build" : "afterok" }
      }
    }
  },
