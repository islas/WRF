{
  "submit_options" :
  {
    "queue"     : "economy",
    "timelimit" : "00:15:00",
    "arguments" : 
    {
      ".*build.*::args_nesting"     : [ "-n", "1" ],
      ".*build.*::args_configopt"   : [ "-o", "-d" ],
      ".*testcase.*::global_env"   : [ "-e", "DATA=/path/to/data" ],
      ".*testcase-mpi.*::exec_mpi" : [ "-p", "mpirun -np 4" ]
    },
    "polaris"   :
    {
      "submission" : "LOCAL",
      "arguments"  :
      {
        ".*testcase.*::global_env"     :
        [ 
          "-e", "DATA=/mnt/s/github/data/wrf/regtest/Data",
          "-e", "LD_LIBRARY_PATH=$LD_LIBRARY_PATH:$NETCDF/lib"
        ]
      }
    },
    "cheyenne"  :
    {
      "submission" : "PBS",
      "resources"  : "-l select=1:ncpus=4",
      "arguments"  : 
      {
        ".*testcase.*::global_env"     :
        [ 
          "-e", "DATA=/glade/work/aislas/github/data/wrf/regtest/Data/",
          "-e", "LD_LIBRARY_PATH=$LD_LIBRARY_PATH:$NETCDF/lib"
        ],
        "global_modules" : [ "gnu", "netcdf" ],
        ".*mpi.*::mpi_module" : [ "openmpi/4.0.3" ]
      }
    }
  },
  "em_real0" :
  {
    "submit_options" :
    {
      "timelimit" : "00:25:00",
      "arguments" :
      {
        ".*testcase.*::args_namelist" : 
        [ 
          "-c", "test/em_real",
          "-b", "real.exe",
          "-n", "namelist.input.3dtke",
          "-d", "$DATA/em_real",
          "-e", "NAMELIST_DIR=em_real"
        ]
      }
    },
    "steps" :
    {
      "build-serial" :
      {
        "command"      : ".ci/tests/build.sh",
        "arguments"    : [ "-c", "32", "-b", "em_real -j 4" ]
      },
      "testcase-serial" :
      {
        "submit_options" : { "cheyenne" : { "resources" : "-l select=1:ncpus=1" } },
        "command"      : ".ci/tests/runNamelists.sh",
        "arguments"    : [ "-f", ".ci/tests/SCRIPTS/Namelists/weekly/$NAMELIST_DIR/SERIAL/", "-s", "serial_results" ],
        "dependencies" : { "build-serial" : "afterok" }
      },
      "build-omp" :
      {
        "command"      : ".ci/tests/build.sh",
        "arguments"    : [ "-c", "33", "-b", "em_real -j 4" ],
        "dependencies" : { "testcase-serial" : "afterok" }
      },
      "testcase-omp" :
      {
        "submit_options" : { "arguments" : { "env" : [ "-e", "OMP_NUM_THREADS=4" ] }, "cheyenne" : { "resources" : "-l select=1:ncpus=4:ompthreads=4" } },
        "command"      : ".ci/tests/runNamelists.sh",
        "arguments"    : [ "-f", ".ci/tests/SCRIPTS/Namelists/weekly/$NAMELIST_DIR/OPENMP/", "-i", "serial_results" ],
        "dependencies" : { "build-omp" : "afterok" }
      },
      "build-mpi" :
      {
        "command"      : ".ci/tests/build.sh",
        "arguments"    : [ "-c", "34", "-b", "em_real -j 4" ],
        "dependencies" : { "testcase-omp" : "afterok" }
      },
      "testcase-mpi" :
      {
        "submit_options" : { "cheyenne" : { "resources" : "-l select=1:ncpus=4:mpiprocs=4" } },
        "command"      : ".ci/tests/runNamelists.sh",
        "arguments"    : [ "-f", ".ci/tests/SCRIPTS/Namelists/weekly/$NAMELIST_DIR/MPI/", "-i", "serial_results" ],
        "dependencies" : { "build-mpi" : "afterok" }
      }
    }
  },
